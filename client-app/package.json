		"axios": "^1.0.0",
		"dayjs": "^1.11.5",
		"formik": "^2.2.9",
		"framer-motion": "^7.5.2",
		"mobx": "^6.6.2",
		"mobx-react-lite": "^3.4.0",
		"react": "^18.2.0",
		"react-calendar": "^3.9.0",
