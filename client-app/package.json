	"eslintIgnore": [
		"/node_modules",
		"/build",
		"/dist",
		"/public/build"
	],
	"engines": {
		"node": ">=16"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
